import pandas as pd
import numpy as np
# Variables del modelo
NX = input("Número de variables de decisisón\n")
NX = int(NX)
# Función objetivo
print("Ingrese datos de función objetivo\n")
objective = []
for i in range(NX):
   f = input(f"Coeficiente de X{i + 1}\n")
   f = float(f)
   objective.append(f)
print(objective)
NR = input("Número de restriciones\n")
NR = int(NR)
Tableau = [ ]
# Restriciones
for i in range(NR):
    Tableau.append([])
    for j in range(NX + 1):
      if j == NX: 
         xv = input(f"LD{i + 1}\n")
      else : 
          xv = input(f"Coeficiente de X{j + 1}\n")
      xv = float(xv)
      Tableau[i].append(xv)
Tableau = np.array(Tableau)
# Tableau = np.insert(Tableau,0,objective)
Tableau
TableauMin = pd.DataFrame(Tableau)
TableauMin
